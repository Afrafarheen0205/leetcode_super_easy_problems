class Solution(object):
    def arraySign(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        pro=1
        for i in nums:
            if i==0:
                return 0
            pro*=i
        if(pro>=1):
            return 1
        elif(pro<1):
            return -1
        else:
            return 0
        
